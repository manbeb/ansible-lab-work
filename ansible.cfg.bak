[defaults]
# Указываем путь к нашему динамическому инвентарю по умолчанию
inventory = ./inventories/dynamic_inventory.py

# Указываем Ansible, где искать роли
roles_path = ./roles

# Плагины
filter_plugins = ./filter_plugins # Указали путь к плагинам.

# Полезные настройки для удобства
host_key_checking = False
retry_files_enabled = False
